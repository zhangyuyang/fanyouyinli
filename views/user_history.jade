          input#job.job(name='job' ,type='text',placeholder="请输入你所在部门和职位")
          textarea#say_some.say_some(name='say_some',placeholder="说点什么吧")
          a#word_sub.user_history_button(href='#',type='hide')
          input#work_creattime(value='', type='hidden')


    .studed
