      a#modiy_password.modiy_password(href="#") 修改密码
  .user_history_center
    .worked
      if worked
        -for(var i = 0; i < worked.length; i++)
          form#work_preview.worked_preview(method='post',action='/save_worked',style="display : inline-block")
            .worked_left
              span(style='position : absolute; color : red; top : 3px') *
              span 单位名称:
              span 工作时间:
              span 部门/职位:
              span(style='margin-top : 40px') 多说几句:
            .worked_right
              h5#company_name1.preview_list #{worked[i].company_name}
              h5#work_year_s1.work_year #{worked[i].work_year_s}
              span(style='position : absolute; top : 50px; left : 82px') 年
              h5#work_year_e1.work_year #{worked[i].work_year_e}
              span(style='position : absolute; top : 50px; left : 175px') 年
              h5#job1.preview_list #{worked[i].job}
              h5#say_some1.say_some_list #{worked[i].say_some}
              a#work_modify_botton1.modify_bottom(href='#')
              a#work_delete_botton1.delete_bottom(href='#')
              input#work_creattime1(value='#{worked[i].creat_time}', type='hidden') 
      //- 克隆的prewiew
      form#work_preview.worked_preview(method='post',action='/save_worked')
        .worked_left
