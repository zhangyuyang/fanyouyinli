          a#work_modify_botton1.modify_bottom(href='#')
          a#work_delete_botton1.delete_bottom(href='#')
          input#work_creattime1(value='', type='hidden')

      //- 克隆的work_save
      form#work_save.work_save(method='post',action='/save_worked')
        .worked_left
          span(style='position : absolute; color : red; top : 3px') *
          span 单位名称:
          span 工作时间:
          span 部门/职位:
          span(style='margin-top : 40px') 多说几句:
        .worked_right
          input#company_name2.company_name(name='company_name',type='text',placeholder="请输入完整的公司名称")
          select#work_year_s2
            option(value='null') 请选择
            - for (var x = 0; x < 30; x++)
              option(value='#{2014 - x}') #{2014 - x}
          span(style='position : absolute; top : 50px; left : 82px') 年
          select#work_year_e2
            option(value='null') 请选择
            - for (var x = 0; x < 30; x++)
              option(value='#{2014 - x}') #{2014 - x}
          span(style='position : absolute; top : 50px; left : 175px') 年
          input#job2.job(name='job' ,type='text',placeholder="请输入你的职位")
          textarea#say_some2.say_some(name='say_some',placeholder="说点什么吧")
          a#work_save_button.work_save_button(href='#')
          input#work_creattime2(value='', type='hidden')

      form#worked(method='post',action='/save_worked')
        .worked_left
          span(style='position : absolute; color : red; top : 3px') *
