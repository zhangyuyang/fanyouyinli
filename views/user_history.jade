include head
.my_attraction
  .navigation(style='border-bottom : 0px')
    label#dinner_party.lab1
    label#singer.lab2(style='background-position: 0px -38px')
    label#attractive.lab3
    label#my_attractive.lab4
  .userinfo_nav
    .userinfo_navtxt
      a#set_photo.set_photo(href="#") 设置头像
      a#set_userinfo.set_userinfo(href="#") 基本信息
