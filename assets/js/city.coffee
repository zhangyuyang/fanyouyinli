window.onload = ->
  i = undefined
  j = undefined
  str = strCity
  t1 = str.split(";")
  t2 = undefined
  objprovince = document.getElementById("province")
  i = 0
  while i < t1.length
    t2 = t1[i].split(",")
    oItem = document.createElement("OPTION")
    oItem.text = t2[0]
    oItem.value = t2[0]
    objprovince.options.add oItem
    if t2[0] is ""
      oItem.selected = true
      fillcity t2[0]
    i++
  objprovince.onchange = ->
    fillcity @value
    return

  return