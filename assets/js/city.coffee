fillcity = (p) ->
  i = undefined
  j = undefined
  objcity = document.getElementById("city")
  i = objcity.options.length - 1
  while i >= 1
    objcity.removeChild objcity.options[i]
    i--
  str = strCity
  t1 = str.split(";")
  t2 = undefined
  i = 0
  while i < t1.length
    t2 = t1[i].split(",")
    if t2[0] is p
      j = 1
      while j < t2.length
        oItem = document.createElement("OPTION")
        oItem.text = t2[j]
        oItem.value = t2[j]
        objcity.options.add oItem
        oItem.selected = true  if oItem.text is ""
        j++
    i++
  return


